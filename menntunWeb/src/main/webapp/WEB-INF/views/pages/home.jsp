                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Customer Code</label>
                      <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter your Customer Code">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Address Line 1</label>
                      <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter Your Address">
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Address Line 2</label>
                      <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter Your Address">
                    </div>
                     <!-- textarea -->
                    <div class="form-group">
                      <label>Description</label>
                      <textarea class="form-control" rows="3" placeholder="Enter your Description"></textarea>
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Account Type</label>
                      <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Enter Bank Account Type">
